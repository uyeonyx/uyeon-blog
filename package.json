{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.4.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "cross-env INIT_CWD=$PWD NODE_OPTIONS='--no-deprecation' next dev",
    "build": "contentlayer2 build && cross-env INIT_CWD=$PWD NODE_OPTIONS='--no-deprecation --experimental-json-modules' next build && cross-env NODE_OPTIONS='--no-deprecation --experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true NODE_OPTIONS='--no-deprecation' next build",
    "lint": "biome check --write .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@iconify-json/solar": "^1.2.5",
    "@iconify/react": "^6.0.2",
    "@next/bundle-analyzer": "16.0.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "contentlayer2": "0.5.8",
    "esbuild": "0.27.0",
    "framer-motion": "^12.23.24",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "image-size": "2.0.2",
    "katex": "^0.16.25",
    "next": "16.0.3",
    "next-contentlayer2": "0.5.8",
    "next-themes": "^0.4.6",
    "pliny": "0.4.1",
    "postcss": "^8.5.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.3.1",
    "rehype-katex": "^7.0.1",
    "rehype-katex-notranslate": "^1.1.4",
    "rehype-preset-minify": "7.0.1",
    "rehype-prism-plus": "^2.0.1",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-github-blockquote-alert": "^2.0.0",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "unist-util-visit": "^5.0.0"
