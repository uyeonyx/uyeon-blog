  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@svgr/webpack": "^8.1.0",
    "@types/mdx": "^2.0.13",
    "@types/react": "^19.2.6",
    "cross-env": "^10.1.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.23.0"
}
